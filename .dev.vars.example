# pnpm gen:env && Copy-Item .generated/cloud-secrets.env .dev.vars

# 必填：云签名私钥（JWK 或 JWK Set）
CLOUD_PRIVATE_KEYS_JSON={"np-cloud-ed25519-2026-01":{"kty":"OKP","crv":"Ed25519","x":"<public>","d":"<private>","kid":"np-cloud-ed25519-2026-01"}}

# 必填：云公钥 JWKS
